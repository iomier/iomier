---
import BaseHead from '@components/BaseHead.astro';

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en" class="h-full dark:bg-gray-800 ">
	<head>
		<BaseHead title={title} description={'des'}/>
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="flex flex-col min-h-full">
		<slot />
	</body>
</html>
